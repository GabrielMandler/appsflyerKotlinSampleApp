<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <TextView
            android:text="@string/oaoa"
            android:layout_width="208dp"
            android:layout_height="252dp"
            android:id="@+id/oaoaDataStr"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.497"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/conversionDataTitle"
    />
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/kotlin_sample_app"
            app:layout_constraintHorizontal_bias="0.488" android:id="@+id/textView2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/conversionDataTitle2" app:layout_constraintVertical_bias="0.129"/>
    <Button
            android:text="@string/simple_in_app_events"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/simpleInAppEventsBtn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.497" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/customInAppEventsBtn"/>
    <TextView
            android:text="@string/deepLink"
            android:layout_width="219dp"
            android:layout_height="108dp"
            android:id="@+id/deepLinkDataStr"
            app:layout_constraintBottom_toTopOf="@+id/simpleInAppEventsBtn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="36dp"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/conversionDataTitle"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/deepLinkDataStr" android:text="Deep link data"/>
    <TextView
            android:text="On app open attribution"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/conversionDataTitle2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/oaoaDataStr"/>
    <Button
            android:text="Custom in app event"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/customInAppEventsBtn" android:layout_marginBottom="44dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.502" app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>